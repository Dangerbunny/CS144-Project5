<web-app id="simple" version="2.4">
	
	<context-param>
	    <param-name>listenPort_https</param-name>
	    <param-value>8443</param-value>
	</context-param>
	<context-param>
	    <param-name>listenPort_http</param-name>
	    <param-value>1448</param-value>
	</context-param>
	
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ItemServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.ItemServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItemServlet</servlet-name>
        <url-pattern>/item</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProxyServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.ProxyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProxyServlet</servlet-name>
        <url-pattern>/suggest</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CardServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.CardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CardServlet</servlet-name>
        <url-pattern>/processCard</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
